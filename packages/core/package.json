{
  "name": "@graphiteds/core",
  "version": "1.0.2",
  "description": "Base components for the Graphite Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/way2web/graphite-design-system.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "components/",
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "npm run clean && stencil build --docs --docs-json dist/docs.json",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "clean": "node scripts/clean.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@stencil/vue-output-target": "^0.5.0",
    "@types/jest": "^26.0.23",
    "@types/puppeteer": "2.0.1",
    "@types/resize-observer-browser": "^0.1.5",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.1.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@stencil/core": "^2.6.0"
  }
}
