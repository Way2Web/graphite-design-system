{
  "name": "@graphiteds/react",
  "version": "1.5.1",
  "description": "React specific wrapper for the components of the Graphite Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/paqtcom/graphite-design-system.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run copy && npm run compile",
    "clean": "rimraf dist && rimraf dist-transpiled",
    "compile": "npm run tsc && rollup -c",
    "test": "echo 'react no tests yet'",
    "tsc": "tsc -p .",
    "copy": "node ./scripts/copy-css.js",
    "sync": "sh ./scripts/sync.sh"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/",
    "css/"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.14",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "jest": "^26.4.0",
    "np": "^6.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.4",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@graphiteds/core": "^1.5.0",
    "tslib": "*"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "sideEffects": [
    "css/*.css"
  ]
}
